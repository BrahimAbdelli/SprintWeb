        </tbody>
    </table>


            <a href="{{ path('offre_index') }}" class="btn btn-success">Back to the list</a>

            <a href="{{ path('offre_edit', { 'id': offrePromotion.id }) }}" class="btn btn-success">Edit</a>

            {{ form_start(delete_form) }}
