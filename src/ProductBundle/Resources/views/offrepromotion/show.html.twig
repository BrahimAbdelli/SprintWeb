{% extends 'back.html.twig' %}
{% block content %}
<div table class="table table-striped">
    <h1>Offrepromotion</h1>

    <table>
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ offrePromotion.id }}</td>
        </tr>
        <tr>
            <th>Pourcentage</th>
            <td>{{ offrePromotion.pourcentage }}</td>
        </tr>
        <tr>
            <th>Datedebut</th>
            <td>{{ offrePromotion.dateDebut |date}}</td>
        </tr>
        <tr>
